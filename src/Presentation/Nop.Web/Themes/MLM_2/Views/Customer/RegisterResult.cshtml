@model RegisterResultModel
@{
    Layout = "_ColumnsOne";

    //title
    NopHtml.AddTitleParts(T("PageTitle.Register").Text);
    //page class
    NopHtml.AppendPageCssClassParts("html-registration-result-page");
    NopHtml.AppendPageCssClassParts("html-registration-page");

}
<div class="page registration-result-page">
    <div class="page-title">
        <h1>@T("Account.Register.Button")</h1>
    </div>
    <div class="page-body">
        <div class="container">
            <div class="result">
                @Model.Result
            </div>
            <div class="buttons">
                <a href="/" class="button-1 register-continue-button">Continue</a>
            </div>
        </div>
        
    </div>
</div>